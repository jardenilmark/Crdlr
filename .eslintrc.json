{
	"extends": ["standard", "plugin:react/recommended", "plugin:prettier/recommended"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"react/prop-types": 0,
		"quotes": ["error", "single"],
		"jsx-quotes": ["error", "prefer-single"]
	},
	"settings": {
		"react": {
			"version": "^16.6.1"
		}
	}
}
